{
  "name": "@lenne.tech/nest-server-starter",
  "version": "8.1.0",
  "description": "Starter for lenne.Tech Nest Server",
  "keywords": [
    "starter",
    "lenne.Tech",
    "node",
    "nest",
    "server",
    "mongodb",
    "graphql",
    "typescript"
  ],
  "author": "Kai Haase",
  "homepage": "http://lenne.tech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lenneTech/nest-server-starter"
  },
  "bugs": {
    "url": "https://github.com/lenneTech/nest-server-starter/issues"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && npm run copy:assets && npm run copy:meta && npm run copy:migrations && npm run copy:images",
    "copy:assets": "cpy --parents --cwd=./src  assets/**/* ../dist",
    "copy:meta": "rimraf .dist/meta.json && cpy ./package.json --rename=meta.json ./dist/",
    "copy:migrations": "cp -R ./migrations ./dist/migrations && cp -R ./migrations-utils ./dist/migrations-utils && cp -R ./tsconfig.json ./dist/tsconfig.json",
    "copy:images": "mkdir -p ./dist/public && cp -R ./public/images ./dist/public/images",
    "format": "prettier --write 'src/**/*.ts' --ignore-unknown",
    "format:check": "prettier --check 'src/**/*.ts' --ignore-unknown",
    "format:staged": "npx pretty-quick --staged",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache",
    "migrate:create": "migrate create --template-file ./migrations-utils/template.ts --migrations-dir=\"./migrations\" --compiler=\"ts:./migrations-utils/ts-compiler.js\"",
    "migrate:up": "migrate --store=./migrations-utils/migrate.js --migrations-dir=\"./migrations\" --compiler=\"ts:./migrations-utils/ts-compiler.js\" up",
    "migrate:develop:up": "NODE_ENV=develop migrate --store=./migrations-utils/migrate.js --migrations-dir=\"./migrations\" --compiler=\"ts:./migrations-utils/ts-compiler.js\" up",
    "migrate:test:up": "NODE_ENV=test migrate --store=./migrations-utils/migrate.js --migrations-dir=\"./migrations\" --compiler=\"ts:./migrations-utils/ts-compiler.js\" up",
    "migrate:preview:up": "NODE_ENV=preview migrate --store=./migrations-utils/migrate.js --migrations-dir=\"./migrations\" --compiler=\"ts:./migrations-utils/ts-compiler.js\" up",
    "migrate:prod:up": "NODE_ENV=production migrate --store=./migrations-utils/migrate.js --migrations-dir=\"./migrations\" --compiler=\"ts:./migrations-utils/ts-compiler.js\" up",
    "prepare": "husky install",
    "prepublishOnly": "npm run format && npm run lint && npm run test:e2e",
    "prestart:prod": "rimraf dist && npm run build",
    "preversion": "npm run lint",
    "reinit": "rimraf package-lock.json && rimraf node_modules && npm cache clean --force && npm i && npm run test:e2e",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "start": "npm run migrate:up && nodemon",
    "start:local": "nodemon --host=0.0.0.0",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:dev": "nodemon",
    "start:nodemon": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:prod": "./node_modules/.bin/grunt productive",
    "stop": "./node_modules/.bin/pm2 delete nest",
    "test": "NODE_ENV=development jest",
    "test:cov": "NODE_ENV=development jest --coverage",
    "test:debug": "NODE_ENV=development node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=development jest --config jest-e2e.json",
    "test:e2e-cov": "NODE_ENV=development jest --config jest-e2e.json --coverage",
    "test:watch": "NODE_ENV=development jest --watch",
    "update": "node extras/sync-packages.mjs"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@apollo/gateway": "0.50.1",
    "@lenne.tech/nest-server": "8.1.0",
    "@nestjs/apollo": "10.0.9",
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@nestjs/graphql": "10.0.9",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/mongoose": "9.0.3",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.4",
    "@nodepit/migrate-state-store-mongodb": "3.0.0",
    "@types/ejs": "3.1.0",
    "@types/lodash": "4.14.182",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.25",
    "@types/node-mailjet": "3.3.8",
    "@types/nodemailer": "6.4.4",
    "@types/passport": "1.0.7",
    "apollo-server-core": "3.6.7",
    "apollo-server-express": "3.6.7",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "ejs": "3.0.2",
    "graphql": "16.3.0",
    "graphql-subscriptions": "2.0.0",
    "json-to-graphql-query": "2.2.3",
    "light-my-request": "4.9.0",
    "lodash": "4.17.21",
    "migrate": "1.8.0",
    "mongodb": "4.5.0",
    "mongoose": "6.3.0",
    "multer": "1.4.4",
    "node-mailjet": "3.3.11",
    "nodemailer": "6.7.3",
    "nodemon": "2.0.15",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5"
  },
  "devDependencies": {
    "@nestjs/testing": "8.4.4",
    "@types/jest": "27.4.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "coffeescript": "2.6.1",
    "cpy-cli": "4.1.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "find-file-up": "2.0.1",
    "grunt": "1.5.2",
    "grunt-bg-shell": "2.3.3",
    "grunt-contrib-clean": "2.0.1",
    "grunt-contrib-watch": "1.1.0",
    "grunt-sync": "0.8.2",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "pm2": "5.2.0",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "semver": "7.3.7",
    "standard-version": "9.3.2",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-morph": "14.0.0",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    }
  },
  "main": "dist/main.js",
  "files": [
    "dist"
  ]
}
